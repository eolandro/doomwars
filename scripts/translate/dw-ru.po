#: g:/Temp/newdw1/dwtest1/scripts/hell/buttons.lua:711
msgid "Research ~!zombification"
msgstr "Исследование зомбирования"

#: g:/Temp/newdw1/dwtest1/scripts/hell/buttons.lua:711
